{
  "name": "sceneyard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "opennext:build": "npx @opennextjs/cloudflare build",
    "preview": "npm run opennext:build && npx wrangler dev",
    "deploy": "npx wrangler deploy",
    "start": "next start",
    "lint": "next lint",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "migrations-local": "npx wrangler d1 migrations apply sceneyard --local",
		"migrations-prod": "npx wrangler d1 migrations apply sceneyard --remote"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.3.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.546.0",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.21",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.43.0"
  }
}
